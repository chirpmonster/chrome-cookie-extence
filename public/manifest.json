{
  "manifest_version": 2,
  "name": "Chrome Cookie携带问题",
  "version": "0.2.1",
  "description": "Chrome 90+的cookie携带问题",
  "icons": {
    "16": "icons/icon64.png",
    "32": "icons/icon64.png",
    "48": "icons/icon64.png",
    "128": "icons/icon64.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "解决Chrome浏览器Cookie携带问题",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://*/",
    "cookies"
  ],
  "content_scripts": [

  ]
}